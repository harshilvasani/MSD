<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="resources.PublicationRecord.Paper" table="paper" catalog="dblp">
    
    	<id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity" />
        </id> 
        
        <property name="paperKey" type="string">
            <column name="paper_key" length="500" not-null="true"/>
        </property>
        
         <property name="title" type="string">
            <column name="title" length="500" not-null="true"/>
        </property>
        
        <property name="year" type="java.lang.Integer">
            <column name="year"/>   
        </property>
        
        <property name="conference" type="string">
            <column name="conference" length="500" not-null="true"/>
        </property>
               
    </class>
</hibernate-mapping>
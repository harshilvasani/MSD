<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="resources.PublicationRecord.Citation" table="citation" catalog="dblp">
    
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity" />
        </id> 
        
        <property name="paperCitedKey" type="string">
            <column name="paper_cited_key" length="500" not-null="true"/>           
        </property>
        
        <property name="paperCiteKey" type="string">
            <column name="paper_cite_key" length="500" not-null="true"/>
        </property>
               
    </class>
</hibernate-mapping>